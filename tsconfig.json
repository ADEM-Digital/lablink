{
  "include": [
    "./src/**/*",
    "src/models/**/*",
    "./database/**/*",
    "src/models/**/*",
    "."
  ],
  "exclude": [
    "node_modules",
    "dist"
  ],
  "compilerOptions": {
   "target": "ES2016",
   "module": "CommonJS",
   "rootDir": "./src",
   "outDir": "./dist",
   "esModuleInterop": true,
   "forceConsistentCasingInFileNames": true,
   "strict": true,
   "skipLibCheck": true
  }
}
